<h1>Listing assets</h1>

<table>
  <tr>
    <th>Imagem file name</th>
    <th>Imagem content type</th>
    <th>Imagem file size</th>
    <th>Imagem updated at</th>
    <th>Preco</th>
    <th>Nome</th>
    <th>Descricao</th>
    <th>Url</th>
    <th>Public</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assets.each do |asset| %>
  <tr>
    <td><%= asset.imagem_file_name %></td>
    <td><%= asset.imagem_content_type %></td>
    <td><%= asset.imagem_file_size %></td>
    <td><%= asset.imagem_updated_at %></td>
    <td><%= asset.preco %></td>
    <td><%= asset.nome %></td>
    <td><%= asset.descricao %></td>
    <td><%= asset.url %></td>
    <td><%= asset.public %></td>
    <td><%= link_to 'Show', asset %></td>
    <td><%= link_to 'Edit', edit_asset_path(asset) %></td>
    <td><%= link_to 'Destroy', asset, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Asset', new_asset_path %>
