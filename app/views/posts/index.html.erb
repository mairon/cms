<h1>Listing posts</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Descricao</th>
    <th>Public</th>
    <th>Url video</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.nome %></td>
    <td><%= post.descricao %></td>
    <td><%= post.public %></td>
    <td><%= post.url_video %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
